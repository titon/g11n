	"description": "The Titon g11n package handles the localization and internationalization of your application.",
	"keywords": ["titon", "g11n", "l10n", "i18n", "locale", "intl", "translate", "message"],
	"homepage": "https://github.com/titon/Titon.G11n",
	"license": "BSD-2-Clause",
	"authors": [
		{
			"name": "Miles Johnson",
			"homepage": "http://milesj.me"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"titon/common": "*",
		"titon/utility": "*",
		"titon/io": "*"
	},
	"require-dev": {
		"titon/cache": "*"
	},
	"suggest": {
		"titon/cache": "Cache the message bundles using a Storage engine"
